# Boxing Gym Agent - Deployment Configuration Template
# Copy this file to deploy-config.env and update with your actual values

# Google API Configuration
GOOGLE_REDIRECT_URI=http://localhost:8080/oauth2callback

# Gmail Configuration
GMAIL_QUERY=from:your_boxing_gym@gmail.com subject:class registration

# Boxing Gym Configuration
BOXING_GYM_EMAIL=your_boxing_gym@gmail.com
BOXING_GYM_NAME=Your Boxing Gym

# Calendar Configuration
CALENDAR_ID=primary
EVENT_DURATION_MINUTES=60
TIMEZONE=America/New_York

# LLM Configuration
LLM_PROVIDER=openai
LLM_MODEL=gpt-4-turbo-preview

# Agent Configuration
CHECK_INTERVAL_MINUTES=5
LOG_LEVEL=INFO
MAX_EMAILS_PER_CHECK=10

# Processing Configuration
CONFIDENCE_THRESHOLD=0.7
ENABLE_AUTO_REGISTRATION=false
ENABLE_CALENDAR_CREATION=true
