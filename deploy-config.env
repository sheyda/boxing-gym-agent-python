# Boxing Gym Agent - Deployment Configuration
# This file contains non-sensitive configuration values for Cloud Run deployment

# Google API Configuration
GOOGLE_REDIRECT_URI=http://localhost:8080/oauth2callback

# Gmail Configuration  
GMAIL_QUERY=subject:"Thanks for filling out this form: Boxing Class Registration" -label:boxing-gym-processed
# Boxing Gym Configuration
BOXING_GYM_EMAIL=dreamlandboxing7@gmail.com
BOXING_GYM_NAME=Dreamland Boxing

# Calendar Configuration
CALENDAR_ID=primary
EVENT_DURATION_MINUTES=60
TIMEZONE=America/Los_Angeles

# LLM Configuration
LLM_PROVIDER=openai
LLM_MODEL=gpt-5-nano

# Agent Configuration
CHECK_INTERVAL_MINUTES=5
LOG_LEVEL=INFO
MAX_EMAILS_PER_CHECK=10

# Processing Configuration
CONFIDENCE_THRESHOLD=0.7
ENABLE_AUTO_REGISTRATION=false
ENABLE_CALENDAR_CREATION=true
